{
  "table_name": "test_data",
  "description": "Sample table for performance testing",
  "fields": {
    "id": {
      "type": "SERIAL",
      "primary_key": true,
      "description": "Auto-incrementing primary key"
    },
    "user_id": {
      "type": "INTEGER",
      "nullable": false,
      "description": "User identifier"
    },
    "name": {
      "type": "VARCHAR(100)",
      "nullable": false,
      "description": "User or item name"
    },
    "email": {
      "type": "VARCHAR(255)",
      "nullable": true,
      "description": "Email address"
    },
    "age": {
      "type": "INTEGER",
      "nullable": true,
      "description": "Age in years"
    },
    "metadata": {
      "type": "JSONB",
      "nullable": true,
      "description": "Additional metadata as JSON"
    },
    "score": {
      "type": "DECIMAL(10,2)",
      "nullable": true,
      "description": "Performance or rating score"
    },
    "is_active": {
      "type": "BOOLEAN",
      "default": true,
      "description": "Active status flag"
    },
    "created_at": {
      "type": "TIMESTAMP",
      "default": "NOW()",
      "description": "Record creation timestamp"
    }
  }
}